  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/seed.command.ts",
    "seed:clean": "ts-node -r tsconfig-paths/register src/database/seeds/seed.command.ts --clean",
    "seed:dev": "ts-node -r tsconfig-paths/register src/database/seeds/seed.command.ts --env=development",
    "seed:test": "ts-node -r tsconfig-paths/register src/database/seeds/seed.command.ts --env=test --clean"
  },
  "dependencies": {
"@aws-sdk/client-s3": "^3.775.0",
